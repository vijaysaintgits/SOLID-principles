package com.solid.services;

import com.solid.interfaces.BookingService;

public class BookingSystem {
	
	 private BookingService bookingService;

	    public BookingSystem(BookingService bookingService) {
	        this.bookingService = bookingService;
	    }

	    public void makeBooking(int identifier) {
	        bookingService.book(identifier);
	    }

}
