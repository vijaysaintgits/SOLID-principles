package com.solid.utility;

import com.solid.entity.Hotel;
import com.solid.entity.Restaurant;
import com.solid.interfaces.BookingService;
import com.solid.services.BookingSystem;
import com.solid.services.HotelBookingService;
import com.solid.services.RestaurantBookingService;

public class Main {

	public static void main(String[] args) {
		
		Hotel hotel = new Hotel();
        Restaurant restaurant = new Restaurant();
       

        
        BookingSystem restaurantBookingSystem = new BookingSystem()

        // Example bookings
        hotelBookingSystem.makeBooking(101); // Book room in the hotel
        restaurantBookingSystem.makeBooking(5); // Reserve table in the restaurant

	}

}
